<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" spacing="16.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="aclcbukidnon.com.javafxactivity.controllers.TodoController">
   <children>
      <HBox prefHeight="100.0" prefWidth="200.0" spacing="16.0">
         <children>
            <Button mnemonicParsing="false" onAction="#onCreateClick" prefHeight="42.0" prefWidth="135.0" style="-fx-background-color: #0f62fe;" text="CREATE" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Button>
            <Button layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#onDeleteClick" prefHeight="42.0" prefWidth="135.0" style="-fx-background-color: #0f62fe;" text="DELETE" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Button>
         </children>
      </HBox>
      <ListView fx:id="todoList" onEditStart="#onListEdit" prefHeight="200.0" prefWidth="200.0" />
   </children>
</VBox>
